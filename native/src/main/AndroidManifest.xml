<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.igame.sanguoheros">

    <application
        android:name="com.mu.sanguo.heroes.app.SgApplication"
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@drawable/sunquan"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.mu.sanguo.heroes.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mu.sanguo.heroes.ui.MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>
