msgid ""
"South: Brimhaven\n"
"West: Loneford\n"
"East: Brightport, Lake Laeroth"
msgstr ""
"Sud: Brimhaven\n"
"Ovest: Loneford\n"
"Est: Brightport, Lago Laeroth"

#: conversationlist_signs_v0611.json:sign_waterway9
msgid ""
"West: Loneford\n"
"East: Brightport, Lake Laeroth"
msgstr ""
"Ovest: Loneford\n"
"Est: Brightport, Lago Laeroth"

#: conversationlist_signs_v0611.json:sign_waterway11
